@model IdentityUser
@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>


<dl>
    <dt>UserName</dt>
    <dd>@Model.UserName</dd>

    <dt>Email</dt>
    <dd>@Model.Email</dd>

    <dt>Phonenumber</dt>
    <dd>@Model.PhoneNumber</dd>

    <dt>LockoutEnabled</dt>
    <dd>@Model.LockoutEnabled</dd>

    <dt>AccessFailedCount</dt>
    <dd>@Model.AccessFailedCount</dd>

    <dt>LockoutEnd</dt>
    <dd>@Model.LockoutEnd</dd>

    <dt>Actions</dt>
    <dd>@Html.ActionLink("Roles Management","RolesManagement","Admin", new { id = Model.Id })</dd>

</dl>

